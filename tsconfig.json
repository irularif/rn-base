{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "app/*": ["app/*"],
      "libs/*": ["libs/*"]
    },
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
